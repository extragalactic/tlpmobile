{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": 0,
    "react/jsx-indent-props": [2, 2],    
    "react/jsx-indent": [2, 2],
    "no-underscore-dangle": 0,
    "max-len": 0,
    "react/forbid-prop-types": [0, { "forbid": [] }]    
  },
  "env": {
    "browser": true,
    "node": true
  },  
  "parser": "babel-eslint"
}